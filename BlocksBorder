		for (int x = -500; x <= 500; x++) {
			if (x == -500 || x == 500) {
				for (int z = -500; z <= 500; z++) {
					for (int y = 0; y <= 130; y++) {
						Location loc = new Location(Bukkit.getWorld("world"), x, y, z);
						if (!loc.getChunk().isLoaded())
							loc.getChunk().load();
						if (new Random().nextBoolean()) {
							Schematic.setBlock(loc.getWorld(), loc.getBlockX(), loc.getBlockY(), loc.getBlockZ(), Material.GLASS.getId(), (byte) 0);
						} else {
							Schematic.setBlock(loc.getWorld(), loc.getBlockX(), loc.getBlockY(), loc.getBlockZ(), Material.GLASS.getId(), (byte) 0);
